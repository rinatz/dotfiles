[core]
	editor = vi
	autocrlf = false
	# pager = delta
[gui]
	encoding = utf-8
[alias]
	ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
[ssh]
	variant = ssh
[pull]
	rebase = false
[diff]
	tool = vscode
	colorMoved = default
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
